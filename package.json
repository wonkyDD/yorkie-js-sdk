{
  "name": "yorkie-js-sdk",
  "version": "0.4.18",
  "description": "Yorkie JS SDK",
  "main": "./dist/yorkie-js-sdk.js",
  "typings": "./dist/yorkie-js-sdk.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:proto": "npx buf generate",
    "build:examples": "npm run build --workspace examples",
    "build:ghpages": "mkdir -p ghpages/examples && find examples -name 'dist' -type d -exec sh -c 'cp -r {} ghpages/examples/$(basename $(dirname {}))' \\;",
    "dev": "vite",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:yorkie.dev": "TEST_RPC_ADDR=https://api.yorkie.dev vitest run --coverage",
    "test:watch": "vitest",
    "test:bench": "vitest bench",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yorkie-team/yorkie-js-sdk.git"
  },
  "author": {
    "name": "hackerwins",
    "email": "susukang98@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yorkie-team/yorkie-js-sdk/issues"
  },
  "homepage": "https://github.com/yorkie-team/yorkie-js-sdk#readme",
  "devDependencies": {
    "@bufbuild/buf": "^1.31.0",
    "@bufbuild/protoc-gen-es": "^1.9.0",
    "@connectrpc/protoc-gen-connect-es": "^1.4.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsdom": "^21.1.6",
    "@types/long": "^4.0.2",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-istanbul": "^1.5.2",
    "@vitest/coverage-v8": "^1.5.2",
    "eslint": "^9.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^5.2.10",
    "vite-plugin-commonjs": "^0.10.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.2",
    "vitest-environment-custom-jsdom": "file:test/vitest/env",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.9.0",
    "@connectrpc/connect": "^1.4.0",
    "@connectrpc/connect-web": "^1.4.0",
    "long": "^5.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "workspaces": [
    "examples/*"
  ]
}
